{
    "$schema": "https://purl.org/csv-cubed/qube-config/v1",
    "title": "Covid Infection Survey Data",
    "description": "The number and proportion of the population estimated to have tested positive for COVID-19 in areas across the UK for given weeks during the year.",
    "creator": "https://www.gov.uk/government/organisations/office-for-national-statistics",
    "publisher": "https://www.gov.uk/government/organisations/office-for-national-statistics",
    "columns": {
        "week": {
            "from_template": "week"
        },
        "ons_geog_code": {
            "type": "dimension",
            "label": "Geography"
        },
        "percentage_pop_est": {
            "type": "observations",
            "data_type": "decimal",
            "unit": {
                "label": "Percent",
                "from_existing": "http://qudt.org/vocab/unit/PERCENT" 
            },
            "measure": {
                "label": "Estimated average % of the population testing positive for COVID-19"
            }
        },
        "percentage_pop_est_lci": {
            "type": "observations",
            "data_type": "decimal",
            "unit": {
                "label": "Percent",
                "from_existing": "http://qudt.org/vocab/unit/PERCENT"
            },
            "measure": {
                "label": "95% Lower confidence/credible interval for percentage"
            }
        },
        "percentage_pop_est_uci": {
            "type": "observations",
            "data_type": "decimal",
            "unit": {
                "label": "Percent",
                "from_existing": "http://qudt.org/vocab/unit/PERCENT"
            },
            "measure": {
                "label": "95% Upper confidence/credible interval for percentage"
            }
        },
        "pop_est": {
            "type": "observations",
            "data_type": "int",
            "unit": {
                "label": "Number",
                "from_existing": "http://qudt.org/vocab/unit/NUM"
            },
            "measure": {
                "label": "Estimated average number of people testing positive for COVID-19"
            }
        },
        "pop_est_lci": {
            "type": "observations",
            "data_type": "int",
            "unit": {
                "label": "Number",
                "from_existing": "http://qudt.org/vocab/unit/NUM"
            },
            "measure": {
                "label": "95% Lower confidence/credible interval for number"
            }
        },
        "pop_est_uci": {
            "type": "observations",
            "data_type": "int",
            "unit": {
                "label": "Number",
                "from_existing": "http://qudt.org/vocab/unit/NUM"
            },
            "measure": {
                "label": "95% Upper confidence/credible interval for number"
            }
        }
    }
}